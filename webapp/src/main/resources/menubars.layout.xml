                <mb3:named>Inmueble</mb3:named>
                <mb3:serviceAction objectType="simple.InmuebleAdd" id="AddInmueble"/>
                <mb3:serviceAction objectType="simple.InmuebleRepositorio" id="listAll"/>
            </mb3:section>
        </mb3:menu>
